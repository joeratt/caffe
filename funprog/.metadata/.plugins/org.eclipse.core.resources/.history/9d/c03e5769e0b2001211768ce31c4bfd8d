package week5

object HigherOrderListFunctions {
  val nums = List(2,-4,5,7,1)                   //> nums  : List[Int] = List(2, -4, 5, 7, 1) //> Welcome to the Scala worksheet
    val fruits = List("apple", "pineapple", "orange", "banana")
    
    nums filter (x => x > 0)
    nums filterNot (x => x > 0)
    nums partition (x => x > 0)
}